@inproceedings{hoa,
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {The {Hanoi} {Omega}-{Automata} {Format}},
	isbn = {978-3-319-21690-4},
	abstract = {We propose a flexible exchange format for 𝜔ω{\textbackslash}omega -automata, as typically used in formal verification, and implement support for it in a range of established tools. Our aim is to simplify the interaction of tools, helping the research community to build upon other people’s work. A key feature of the format is the use of very generic acceptance conditions, specified by Boolean combinations of acceptance primitives, rather than being limited to common cases such as Büchi, Streett, or Rabin. Such flexibility in the choice of acceptance conditions can be exploited in applications, for example in probabilistic model checking, and furthermore encourages the development of acceptance-agnostic tools for automata manipulations. The format allows acceptance conditions that are either state-based or transition-based, and also supports alternating automata. Open image in new window},
	language = {en},
	booktitle = {Computer {Aided} {Verification}},
	publisher = {Springer International Publishing},
	author = {Babiak, Tomáš and Blahoudek, František and Duret-Lutz, Alexandre and Klein, Joachim and Křetínský, Jan and Müller, David and Parker, David and Strejček, Jan},
	editor = {Kroening, Daniel and Păsăreanu, Corina S.},
	year = {2015},
	keywords = {Acceptance Condition, Atomic Proposition, Boolean Formula, Formal Verification, Model Check},
	pages = {479--486},
}

